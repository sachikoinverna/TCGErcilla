<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TCGErcilla.Views.DistribuidoresView"
             xmlns:viewmodels="clr-namespace:TCGErcilla.ViewModels"
             x:DataType="viewmodels:DistribuidoresViewModel"
             BindingContext="{viewmodels:DistribuidoresViewModel}"
             Title="DistribuidoresView">
    <VerticalStackLayout>
        <Button Text="Ir a crear distribuidor" Command="{Binding GoDistribuidorCrearCommand}"/>
        <Button Text="Ver distribuidores" Command="{Binding GoDistribuidoresCommand}"/>
    </VerticalStackLayout>
</ContentPage>